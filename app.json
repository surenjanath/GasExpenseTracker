{
  "expo": {
    "name": "Gas Tracker",
    "slug": "gas-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gas.tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.gas.tracker"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "gas-tracker",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "640b61d5-d251-48cc-9646-3c8c53b28217"
      }
    },
    "owner": "surenjanath_singh"
  }
}
